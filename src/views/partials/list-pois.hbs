{{#each groupedPois}}
  <h2 class="title is-3">
    {{_id.name}}s
  </h2>
  {{#each pois}}
    <div class="card">
      <header class="card-header">
        <p class="card-header-title">
          {{this.name}}
        </p>
        <button class="card-header-icon" aria-label="more options">
          <span class="icon">
            <i class="fa-solid fa-location-pin"></i>
          </span>
        </button>
      </header>
      <div class="card-content">
          <div class="content">
            {{this.description}}
          </div>
        </div>
        <footer class="card-footer">
          <a href="/poi/{{this._id}}" class="card-footer-item">Open</a>
          <a href="/dashboard/deletepoi/{{this._id}}" class="card-footer-item">Delete</a>
        </footer>
      </div>
    <br>
  {{/each}}
  <hr>
{{/each}}